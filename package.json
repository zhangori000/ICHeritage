{
  "name": "xinying-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "seed:generate": "node ./src/sanity/scripts/generate-seed.mjs",
    "seed:import": "npx sanity dataset import ./src/sanity/seed.ndjson production",
    "seed": "npm run seed:generate && npm run seed:import",
    "predev": "npm run typegen",
    "prebuild": "npm run typegen",
    "typegen": "sanity schema extract --path=./src/sanity/extract.json && sanity typegen generate"
  },
  "dependencies": {
    "@sanity/image-url": "^1.2.0",
    "@sanity/vision": "^4.8.1",
    "next": "15.5.3",
    "next-sanity": "^10.1.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sanity": "^4.8.1",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^10.0.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
